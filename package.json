{
  "name": "todo-list",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint": "eslint --ext .ts,vue --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .ts,vue --ignore-path .gitignore .",
    "prettier": "prettier --check --ignore-path .gitignore .",
    "prettier:fix": "prettier --write --ignore-path .gitignore .",
    "stylelint": "stylelint '**/*.{vue,scss,css}' --ignore-path .gitignore",
    "stylelint:fix": "stylelint '**/*.{vue,scss,css}' --fix --ignore-path .gitignore",
    "test:static": "pnpm prettier && pnpm run lint && pnpm run stylelint",
    "test:static:fix": "pnpm prettier:fix && pnpm run lint:fix && pnpm run stylelint:fix"
  },
  "dependencies": {
    "firebase": "^9.5.0",
    "vue": "^3.2.22"
  },
  "devDependencies": {
    "@types/node": "^16.11.9",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vitejs/plugin-vue": "^1.9.4",
    "@vue/compiler-sfc": "^3.2.22",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.20.0",
    "postcss-html": "^1.3.0",
    "prettier": "^2.4.1",
    "sass": "^1.43.4",
    "stylelint": "^14.1.0",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-config-recommended-vue": "^1.1.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "typescript": "^4.5.2",
    "vite": "^2.6.14",
    "vite-plugin-eslint": "^1.3.0",
    "vite-plugin-windicss": "^1.5.1",
    "vue-tsc": "^0.3.0",
    "windicss": "^3.2.1"
  }
}
